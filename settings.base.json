{
  "hostname": "localhost",
  "url": "http://localhost",
  "logo_source": "logo_source.png",
  "contrib": {
    "css": [
    ],
    "js": [
    ]
  },
  "custom": {
    "css": [
      "css/layouts/**/*.css",
      "jslib/**/*.css",
      "css/<%= package.name %>.normalize.css",
      "css/<%= package.name %>.styles.css"
    ],
    "js": [
