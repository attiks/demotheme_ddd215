      "css/<%= package.name %>.hacks.css": "sass/<%= package.name %>.hacks.scss",
      "css/<%= package.name %>.no-query.css": "sass/<%= package.name %>.no-query.scss",
      "css/<%= package.name %>.normalize.css": "sass/<%= package.name %>.normalize.scss",
      "css/layouts/basic/basic.layout.css": "sass/layouts/basic/basic.layout.scss"
    }
  },
  "pagespeed_paths": ["/"],
  "photobox": {
    "urls": ["http://localhost"]
  },
  "phantomas": {
    "url": "http://localhost"
  },
  "drush": {
    "alias": "@none"
